{
  "name": "obs-copilot",
  "overrides": {
    "type-fest": "3.11.0"
  },
  "_comment": "Pinning type-fest to 3.11.x to satisfy obs-websocket-js@5.0.6 (^3.11.0) while avoiding conflicts observed with newer 3.x releases.",
  "version": "0.0.0",
  "private": true,
  "description": "<div align=\"center\">",
  "homepage": "https://github.com/myrqyry/obs-copilot#readme",
  "bugs": {
    "url": "https://github.com/myrqyry/obs-copilot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myrqyry/obs-copilot.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "src/App.tsx",
  "scripts": {
    "frontend": "vite",
    "backend:setup": "node -e \"const {execSync}=require('child_process'); const isWin=process.platform==='win32'; try{ execSync(isWin? 'python -m venv backend/venv' : 'python3 -m venv backend/venv', {stdio:'inherit'}); }catch(e){ execSync('python -m venv backend/venv', {stdio:'inherit'}); } console.log('backend venv ready');\"",
    "backend:dev": "node -e \"const {execSync, spawn}=require('child_process'); const fs=require('fs'); const isWin=process.platform==='win32'; const py = isWin ? 'backend/venv/Scripts/python.exe' : 'backend/venv/bin/python'; if(!fs.existsSync('backend/venv')){ try{ execSync(isWin? 'python -m venv backend/venv' : 'python3 -m venv backend/venv', {stdio:'inherit'}); }catch(e){ execSync('python -m venv backend/venv', {stdio:'inherit'}); } } try{ execSync(py + ' -m pip install -r backend/requirements.txt', {stdio:'inherit'}); }catch(e){} const child=spawn(py, ['-m','uvicorn','backend.main:app','--reload','--port','8000','--host','127.0.0.1'], {stdio:'inherit'}); child.on('exit', code=>process.exit(code));\"",
    "dev": "concurrently \"npm:frontend\" \"npm:backend:dev\" -n frontend,backend -c cyan,magenta",
    "build": "tsc && vite build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "preview": "vite preview",
    "test": "jest",
    "clean": "node scripts/cleanup.js",
    "check-deps": "node scripts/check-unused-deps.js"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.2.0",
    "@google/genai": "^1.13.0",
    "@heroicons/react": "^2.2.0",
    "@imgly/background-removal": "^1.0.0",
    "@radix-ui/react-toast": "^1.2.15",
    "@streamerbot/client": "^1.11.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.0",
    "gsap": "^3.13.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.515.0",
    "obs-websocket-js": "^5.0.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "serverless-http": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.11",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express-rate-limit": "^8.0.1",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "typescript": "^5.9.2",
    "vite": "^6.0.0",
    "vite-plugin-node-polyfills": "~0.24.0"
  }
}
